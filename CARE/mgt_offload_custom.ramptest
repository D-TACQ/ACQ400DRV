
proc test_work {src blocknum} {
	try {
		set fp [open |[list is_ramp -f 1 <$src 2>@1] r]
		gets $fp result
		puts "mgt_block $blocknum result $result"
		while {[gets $fp result] >= 0} {
			puts $result
		}
		close $fp
	} trap CHILDSTATUS {- opts} {
		puts "exit with error"
		exit 1
	}
}

set work test_work

